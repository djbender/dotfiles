[user]
	name = Derek Bender
	useconfigonly = true
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/local/bin/vim
[log]
	decorate = full
[color]
	ui = true
	diff = auto
	status = auto
	branch = auto
[help]
	autocorrect = 1
[push]
	default = current
[pull]
	rebase = true
[alias]
  branchdate = !git for-each-ref --sort='-authordate:iso8601' --format='  %(authordate:iso8601)%09%(objectname:short)%09%(refname)' refs/heads
  simplelog = !git log --pretty=oneline --abbrev-commit
	ctags = !.git/hooks/ctags
[init]
	templatedir = ~/.git_template
